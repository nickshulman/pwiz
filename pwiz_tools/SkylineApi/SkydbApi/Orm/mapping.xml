<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="SkydbApi.Orm">
	<class name="CandidatePeakGroup">
		<id name="Id">
			<generator class="native" />
		</id>
		<property name="StartTime" />
		<property name="EndTime" />
		<property name="Identified" />
	</class>
	<class name="CandidatePeak">
		<id name="Id">
			<generator class="native" />
		</id>
		<many-to-one name="CandidatePeakGroup" />
		<property name="StartTime" />
		<property name="EndTime" />
		<property name="Area" />
		<property name="BackgroundArea" />
		<property name="Height" />
		<property name="FullWidthAtHalfMax" />
		<property name="PointsAcross" />
		<property name="DegenerateFwhm" />
		<property name="ForcedIntegration" />
		<property name="TimeNormalized" />
		<property name="Truncated" />
		<property name="MassError" />
	</class>
	<class name="ChromatogramData">
		<id name="Id">
			<generator class="native" />
		</id>
		<property name="PointCount" />
		<property name="RetentionTimesData" />
		<property name="IntensitiesData" />
		<property name="MassErrorsData" />
	</class>
	<class name="MsDataFile">
		<id name="Id">
			<generator class="native" />
		</id>
		<property name="FilePath" />
	</class>
	<class name="SpectrumInfo">
		<id name="Id">
			<generator class="native" />
		</id>
		<many-to-one name="MsDataFile" />
		<property name="SpectrumIndex" />
		<property name="SpectrumIdentifier" />
		<property name="RetentionTime" />
	</class>
	<class name="Scores">
		<id name="Id">
			<generator class="native" />
		</id>
	</class>
	<class name="SpectrumList">
		<id name="Id">
			<generator class="native" />
		</id>
		<property name="SpectrumCount" />
		<property name="SpectrumIndexData" />
	</class>
</hibernate-mapping>
